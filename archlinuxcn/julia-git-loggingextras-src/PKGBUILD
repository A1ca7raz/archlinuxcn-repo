sha256sums=('SKIP')

package() {
  cd LoggingExtras.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh LoggingExtras "${pkgdir}" "${pkgname}" julia-git
}
