pkgname=julia-git-loggingextras-src
pkgver=1.0.2
_commit=2f72a099df0803668ba4ef1217227b8996a64f25
pkgrel=2
pkgdesc="LoggingExtras.jl"
url="https://github.com/JuliaLogging/LoggingExtras.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
