pkgname=julia-git-contour-src
pkgver=0.6.2
_commit=0160a4137bab2bb05480a01769b847cf5073caa5
pkgrel=1
pkgdesc="Contour.jl"
url="https://github.com/JuliaGeometry/Contour.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
