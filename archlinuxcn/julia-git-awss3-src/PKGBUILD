sha256sums=('SKIP')

package() {
  cd AWSS3.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh AWSS3 "${pkgdir}" "${pkgname}" julia-git
}
