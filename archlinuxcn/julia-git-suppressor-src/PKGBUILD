sha256sums=('SKIP')

package() {
  cd Suppressor.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh Suppressor "${pkgdir}" "${pkgname}" julia-git
}
