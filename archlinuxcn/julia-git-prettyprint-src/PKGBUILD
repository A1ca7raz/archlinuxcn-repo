sha256sums=('SKIP')

package() {
  cd PrettyPrint.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh PrettyPrint "${pkgdir}" "${pkgname}" julia-git
}
