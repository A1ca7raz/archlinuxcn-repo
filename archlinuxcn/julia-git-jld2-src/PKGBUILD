pkgname=julia-git-jld2-src
pkgver=0.4.37
_commit=17c6d63682d93a0bd077091e7c3b584cba584af4
pkgrel=1
pkgdesc="JLD2.jl"
url="https://github.com/JuliaIO/JLD2.jl.git"
arch=('any')
license=('MIT')
