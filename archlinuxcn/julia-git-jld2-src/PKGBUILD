sha256sums=('SKIP')

package() {
  cd JLD2.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh JLD2 "${pkgdir}" "${pkgname}" julia-git
}
