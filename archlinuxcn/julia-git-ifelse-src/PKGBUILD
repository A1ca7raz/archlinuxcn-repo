pkgname=julia-git-ifelse-src
pkgver=0.1.1
_commit=44604bf115755bc201200cedb0291e2241b400dd
pkgrel=1
pkgdesc="IfElse.jl"
url="https://github.com/SciML/IfElse.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
