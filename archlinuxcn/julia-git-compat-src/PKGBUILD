pkgname=julia-git-compat-src
pkgver=4.9.0
_commit=cc4763c
pkgrel=1
pkgdesc="Compat.jl"
url="https://github.com/JuliaLang/Compat.jl"
arch=('any')
license=('MIT')
makedepends=('git' 'julia-pkg-scripts')
depends=(julia-git)
source=("git+https://github.com/JuliaLang/Compat.jl#commit=$_commit")
md5sums=('SKIP')

package() {
  cd Compat.jl

  . /usr/lib/julia/julia-install-pkg.sh Compat "${pkgdir}" "${pkgname}" julia-git
}
