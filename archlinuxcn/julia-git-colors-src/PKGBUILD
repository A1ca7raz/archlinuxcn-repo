pkgname=julia-git-colors-src
pkgver=0.12.10
_commit=da0045a1de1b2bdf559ed3aa6bf91293f0563736
pkgrel=2
pkgdesc="Colors.jl"
url="https://github.com/JuliaGraphics/Colors.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
