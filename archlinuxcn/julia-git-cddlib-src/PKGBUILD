sha256sums=('SKIP')

package() {
  cd CDDLib.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh CDDLib "${pkgdir}" "${pkgname}" julia-git
}
