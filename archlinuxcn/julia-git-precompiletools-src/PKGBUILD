sha256sums=('SKIP')

package() {
  cd PrecompileTools.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh PrecompileTools "${pkgdir}" "${pkgname}" julia-git
}
