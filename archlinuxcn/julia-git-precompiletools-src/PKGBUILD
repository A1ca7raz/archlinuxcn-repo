pkgname=julia-git-precompiletools-src
pkgver=1.2.0
_commit=325b4699b6984ea7a16fd0fa4fdbf969954d36e8
pkgrel=2
pkgdesc="PrecompileTools.jl"
url="https://github.com/JuliaLang/PrecompileTools.jl.git"
arch=('any')
