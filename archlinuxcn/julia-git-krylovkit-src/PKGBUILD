sha256sums=('SKIP')

package() {
  cd KrylovKit.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh KrylovKit "${pkgdir}" "${pkgname}" julia-git
}
