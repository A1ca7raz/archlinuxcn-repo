pkgname=julia-git-crlibm-src
pkgver=1.0.1
_commit=6a091d8fd25fb205faf4b3785dfa32505602021d
pkgrel=1
pkgdesc="CRlibm.jl"
url="https://github.com/JuliaIntervals/CRlibm.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
