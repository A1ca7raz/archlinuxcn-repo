sha256sums=('SKIP')

package() {
  cd CRlibm.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh CRlibm "${pkgdir}" "${pkgname}" julia-git
}
