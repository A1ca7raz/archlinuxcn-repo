pkgname=julia-git-nameresolution-src
pkgver=0.1.5
_commit=2c7674ec36eba16559111c58e8418f2070260a45
pkgrel=1
pkgdesc="NameResolution.jl"
url="https://github.com/JuliaStaging/NameResolution.jl.git"
arch=('any')
