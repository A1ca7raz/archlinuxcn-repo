sha256sums=('SKIP')

package() {
  cd NameResolution.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh NameResolution "${pkgdir}" "${pkgname}" julia-git
}
