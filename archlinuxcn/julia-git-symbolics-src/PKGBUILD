pkgname=julia-git-symbolics-src
pkgver=5.20.1
_commit=1e55b62f6dae8ed93b3533ceabddff78ef5d7639
pkgrel=1
pkgdesc="Symbolics.jl"
url="https://github.com/JuliaSymbolics/Symbolics.jl.git"
arch=('any')
