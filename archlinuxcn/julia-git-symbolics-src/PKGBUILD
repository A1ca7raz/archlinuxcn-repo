sha256sums=('SKIP')

package() {
  cd Symbolics.jl

  JULIA_INSTALL_SRCPKG=1 . /usr/lib/julia/julia-install-pkg.sh Symbolics "${pkgdir}" "${pkgname}" julia-git
}
