pkgname=julia-git-graphs-src
pkgver=1.9.0
_commit=0fa5f52856f7f779e8f2ca49bc6cee26e5e19b50
pkgrel=1
pkgdesc="Graphs.jl"
url="https://github.com/JuliaGraphs/Graphs.jl.git"
arch=('any')
license=('MIT')
makedepends=(git julia-pkg-scripts)
depends=(julia-git)
